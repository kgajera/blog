    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "g:blog": "hygen blog new",
    "g:component": "hygen component new",
