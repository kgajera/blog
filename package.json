  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@docusaurus/theme-common": "^2.1.0",
    "@giscus/react": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.1.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "cypress": "^10.3.0",
    "dotenv": "^16.0.2",
    "husky": "^8.0.0",
    "hygen": "^6.2.0",
    "hygen-docusaurus": "^1.1.0",
    "lint-staged": ">=10",
